{
  "name": "vite-sitemap",
  "version": "1.6.0",
  "description": "Vite plugin for generating SEO-friendly sitemaps",
  "keywords": [
    "vite",
    "sitemap",
    "vite-plugin",
    "SEO"
  ],
  "author": "Saeed Hosan",
  "license": "MIT",
  "homepage": "https://github.com/saeedhosan/vite-sitemap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saeedhosan/vite-sitemap.git"
  },
  "bugs": {
    "url": "https://github.com/saeedhosan/vite-sitemap/issues"
  },
  "scripts": {
    "build": "tsc",
    "publish": "tsc && npm publish --access public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "vite": "*"
  }
}